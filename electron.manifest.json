{
  "executable": "electronNetTest",
  "splashscreen": {
    "imageFile": ""
  },
  "singleInstance": true,
  "build": {
    "appId": "com.minze.configurator.app",
    "productName": "MinZeBox Configurator",
    "copyright": "Copyright Â© 2019",
    "buildVersion": "1.0.1",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop",
      "buildResources": "../../../build"
    },
    "linux":{
      "target": "deb",
      "executableName": "mbox_configurator",
      "desktop": {
        "Name": "MinzeBox Configurator",
        "Comment": "Configurator for the boxes",
        "Categories": "Network"
      },
      "maintainer": "aitor@aitordev.com",
      "extraResources": [
        {
          "from": "../../../build",
          "to": "./bin/build",
          "filter": [
            "/"
          ]
        }
      ]
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": ["**/*"]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": ["**/*"]
      },
      "**/*"
    ]
  }
}